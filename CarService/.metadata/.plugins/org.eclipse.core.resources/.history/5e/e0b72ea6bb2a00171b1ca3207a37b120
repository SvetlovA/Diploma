(function() {
	var rows = document.querySelectorAll(".table-row"),
		rowChildNodes,
		cellChildNodes,
		i;
	
	for (i = 0; i < rows.length; i++) {
		rowChildNodes = rows[i].children;
		rowChildNodes.forEach(function(rowNode) {
			if (rowNode.getAttribute("class") === "table-cell work-status" &&
					rowNode.innerHtml === "Completed") {
				cellChildNodes = rowChildNodes[childNodes.length - 1].children;
				cellChildNodes.forEach(function(callNode) {
					if (cellNode.getAttribute("class") === "hide-action") {
						cellNode.classList.add("hide");
					}
				});
			}
		});
	}
})();